return {
    'preservim/vim-indent-guides',
    lazy = false,
    priority = 1000,
    init = function()
        vim.g.indent_guides_enable_on_vim_startup = 1
        vim.g.indent_guides_start_level = 1
        vim.g.indent_guides_guide_size = 1
    end,
    config = function() vim.cmd("IndentGuidesEnable") end
}
